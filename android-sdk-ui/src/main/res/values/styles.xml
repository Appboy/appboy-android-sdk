<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
  <style name="Appboy"/>

  <style name="Appboy.Button">
    <!-- Overriding the Holo default behavior -->
    <item name="android:minHeight">0.0dp</item>
    <item name="android:minWidth">0.0dp</item>
    <item name="android:textColor">@color/com_appboy_button</item>
  </style>

  <!-- Custom Push Notification View Styles. We ensure that this is only used for
  Ice Cream Sandwich, so we mark it with targetApi 14 for Lint purposes. -->
  <style name="Appboy.EventContent" tools:targetApi="14">
    <item name="android:textColor">@color/com_appboy_custom_notification_content</item>
    <item name="android:textSize">12sp</item>
  </style>
  <style name="Appboy.EventContent.Title">
    <item name="android:textColor">@color/com_appboy_custom_notification_title</item>
    <item name="android:textSize">16sp</item>
    <item name="android:textStyle">bold</item>
  </style>
  <style name="Appboy.EventContent.Time">
    <item name="android:textSize">10sp</item>
    <item name="android:textColor">@color/com_appboy_custom_notification_time</item>
  </style>

  <!-- In-app Message -->
  <style name="Appboy.InAppMessage">
  </style>
  <style name="Appboy.InAppMessage.Header">
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:padding">0.0dp</item>
    <item name="android:background">@android:color/transparent</item>
    <item name="android:textColor">@color/com_appboy_inappmessage_header_text</item>
    <item name="android:textSize">20.0sp</item>
    <item name="android:lineSpacingMultiplier">1.3</item>
    <item name="android:gravity">center</item>
    <item name="android:textStyle">bold</item>
    <item name="android:layout_centerHorizontal">true</item>
  </style>
  <style name="Appboy.InAppMessage.Message">
    <item name="android:background">@android:color/transparent</item>
    <item name="android:textSize">14.0sp</item>
    <item name="android:lineSpacingMultiplier">1.1</item>
    <item name="android:lineSpacingExtra">0dp</item>
    <item name="android:textColor">@color/com_appboy_inappmessage_text</item>
  </style>
  <style name="Appboy.InAppMessage.ScrollView">
    <item name="android:fillViewport">true</item>
  </style>
  <style name="Appboy.InAppMessage.Image">
  </style>
  <style name="Appboy.InAppMessage.Icon">
    <item name="android:layout_height">50.0dp</item>
    <item name="android:layout_width">50.0dp</item>
    <item name="android:textSize">30.0sp</item>
    <item name="android:padding">0.0dp</item>
    <item name="android:gravity">center</item>
    <item name="android:layout_gravity">center</item>
    <item name="android:background">@drawable/com_appboy_icon_background</item>
    <item name="android:textColor">@color/com_appboy_inappmessage_icon</item>
  </style>
  <style name="Appboy.InAppMessage.Layout">
    <item name="android:layout_height">wrap_content</item>
    <item name="android:padding">0.0dp</item>
  </style>
  <style name="Appboy.InAppMessage.Layout.Image">
  </style>
  <style name="Appboy.InAppMessage.Layout.ButtonAndText">
  </style>
  <style name="Appboy.InAppMessage.Layout.Button">
    <item name="android:layout_width">match_parent</item>
    <item name="android:orientation">horizontal</item>
    <item name="android:weightSum">2.0</item>
  </style>
  <style name="Appboy.InAppMessage.Layout.Text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_centerHorizontal">true</item>
    <item name="android:orientation">vertical</item>
  </style>
  <style name="Appboy.InAppMessage.Button">
    <item name="android:layout_height">44dp</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:textColor">@color/com_appboy_inappmessage_button_text_light</item>
    <item name="android:background">@drawable/com_appboy_inappmessage_button_background</item>
    <item name="android:textSize">14.0sp</item>
    <item name="android:gravity">center</item>
    <item name="android:layout_gravity">center</item>
    <item name="android:singleLine">true</item>
    <item name="android:textStyle">bold</item>
    <item name="android:paddingLeft">12dp</item>
    <item name="android:paddingRight">12dp</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:minWidth">80dp</item>
    <item name="android:layout_weight">1</item>
  </style>
  <style name="Appboy.InAppMessage.CloseButton">
    <item name="android:contentDescription">@string/com_appboy_inappmessage_close_content_description</item>
    <item name="android:layout_height">20dp</item>
    <item name="android:layout_width">20dp</item>
    <item name="android:layout_alignParentRight">true</item>
    <item name="android:background">@drawable/com_appboy_inappmessage_button_close</item>
    <item name="android:layout_marginRight">15dp</item>
    <item name="android:layout_marginTop">15dp</item>
    <item name="android:foreground">?android:attr/selectableItemBackground</item>
  </style>
  <style name="Appboy.InAppMessage.Chevron">
    <item name="android:layout_height">20dp</item>
    <item name="android:layout_width">12dp</item>
    <item name="android:background">@drawable/com_appboy_inappmessage_chevron</item>
    <item name="android:layout_marginRight">20dp</item>
    <item name="android:layout_marginLeft">18dp</item>
    <item name="android:layout_alignParentRight">true</item>
    <item name="android:layout_centerVertical">true</item>
  </style>
  <style name="Appboy.InAppMessage.Frame">
    <item name="android:layout_height">match_parent</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:background">@color/com_appboy_inappmessage_frame_light</item>
  </style>

  <!-- In-App Message Slideup -->
  <style name="Appboy.InAppMessage.Slideup">
    <item name="android:layout_height">60.0dp</item>
    <item name="android:layout_width">match_parent</item>
  </style>
  <!--
  On pre API 19 devices, the slideup won't have the proper bottom
  margin that makes it "float". Adding padding here or changing the height flag
  doesn't work either. This is a confirmed bug in pre API 19
  (https://android.googlesource.com/platform/frameworks/base/+/android-4.2.2_r1.1/core/java/android/widget/RelativeLayout.java#487)
  that is confirmed fixed in API 19
  (https://android.googlesource.com/platform/frameworks/base/+/android-4.4_r1/core/java/android/widget/RelativeLayout.java#498)
  -->
  <style name="Appboy.InAppMessage.Slideup.Container">
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_marginLeft">10dp</item>
    <item name="android:layout_marginRight">10dp</item>
    <item name="android:layout_marginBottom">10dp</item>
    <item name="android:background">@drawable/com_appboy_inappmessage_slideup_background</item>
  </style>
  <style name="Appboy.InAppMessage.Message.Slideup">
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:textColor">@color/com_appboy_inappmessage_text_slideup</item>
    <item name="android:textSize">14.0sp</item>
    <item name="android:maxLines">3</item>
    <item name="android:gravity">start</item>
    <item name="android:layout_gravity">center_vertical</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:layout_toRightOf">@id/com_appboy_inappmessage_slideup_image_layout</item>
    <item name="android:layout_toLeftOf">@id/com_appboy_inappmessage_slideup_chevron</item>
    <item name="android:layout_marginTop">15dp</item>
    <item name="android:layout_marginBottom">15dp</item>
    <item name="android:layout_marginRight">20dp</item>
    <item name="android:paddingBottom">15dp</item>
    <item name="android:padding">0.0dp</item>
    <item name="android:ellipsize">end</item>
  </style>
  <style name="Appboy.InAppMessage.Slideup.ImageContainer">
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:layout_alignParentLeft">true</item>
    <item name="android:layout_marginRight">10dp</item>
    <item name="android:layout_marginLeft">15dp</item>
    <item name="android:layout_marginTop">15dp</item>
    <item name="android:layout_marginBottom">15dp</item>
  </style>
  <style name="Appboy.InAppMessage.Image.Slideup">
    <item name="android:contentDescription">@string/com_appboy_inappmessage_image_content_description</item>
    <item name="android:layout_height">50.0dp</item>
    <item name="android:layout_width">50.0dp</item>
  </style>
  <style name="Appboy.InAppMessage.Icon.Slideup">
    <item name="android:contentDescription">@string/com_appboy_inappmessage_icon_content_description</item>
  </style>
  <style name="Appboy.InAppMessage.Chevron.Slideup">
    <item name="android:layout_marginLeft">0dp</item>
  </style>

