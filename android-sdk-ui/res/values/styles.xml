<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
  <style name="Appboy"/>

  <style name="Appboy.Button">
    <!-- Overriding the Holo default behavior -->
    <item name="android:minHeight">0.0dp</item>
    <item name="android:minWidth">0.0dp</item>
    <item name="android:textColor">@color/com_appboy_button</item>
  </style>

  <!-- Custom Push Notification View Styles. AppboyGcmReceiver ensures that this is only used for Android versions from
   Honeycomb to Ice Cream Sandwich, so we mark it with targetApi 11 for Lint purposes. -->
  <style name="Appboy.EventContent" tools:targetApi="11">
    <item name="android:textColor">@color/com_appboy_custom_notification_content</item>
    <item name="android:textSize">12dp</item>
  </style>
  <style name="Appboy.EventContent.Title">
    <item name="android:textColor">@color/com_appboy_custom_notification_title</item>
    <item name="android:textSize">16dp</item>
    <item name="android:textStyle">bold</item>
  </style>
  <style name="Appboy.EventContent.Time">
    <item name="android:textSize">10dp</item>
    <item name="android:textColor">@color/com_appboy_custom_notification_time</item>
  </style>

  <!-- Feedback -->
  <style name="Appboy.Feedback"/>
  <style name="Appboy.Feedback.Layout">
    <item name="android:background">@color/com_appboy_feedback_form_background</item>
  </style>
  <style name="Appboy.Feedback.NavigationBar">
    <item name="android:padding">4dp</item>
    <item name="android:background">@color/com_appboy_feedback_form_navigation_bar_background</item>
  </style>
  <style name="Appboy.Feedback.NavigationBarCancelButton">
    <item name="android:layout_marginRight">2dp</item>
    <item name="android:text">@string/com_appboy_feedback_form_cancel</item>
    <item name="android:textStyle">bold</item>
  </style>
  <style name="Appboy.Feedback.NavigationBarSendButton">
    <item name="android:layout_marginLeft">2dp</item>
    <item name="android:text">@string/com_appboy_feedback_form_send</item>
    <item name="android:textStyle">bold</item>
  </style>
  <style name="Appboy.Feedback.EmailEditText">
    <item name="android:inputType">textEmailAddress</item>
    <item name="android:hint">@string/com_appboy_feedback_form_email</item>
  </style>
  <style name="Appboy.Feedback.MessageEditText">
    <item name="android:inputType">text|textMultiLine|textCapSentences</item>
    <item name="android:gravity">top</item>
    <item name="android:minLines">5</item>
    <item name="android:maxLines">100</item>
    <item name="android:hint">@string/com_appboy_feedback_form_message</item>
  </style>
  <style name="Appboy.Feedback.IsBugCheckBox">
    <item name="android:text">@string/com_appboy_feedback_form_is_bug</item>
    <item name="android:textColor">@color/com_appboy_feedback_form_is_bug</item>
  </style>

  <!-- Slideup -->
  <style name="Appboy.Slideup">
    <item name="android:padding">8.0sp</item>
    <item name="android:background">@color/com_appboy_slideup_background</item>
  </style>
  <style name="Appboy.Slideup.Message">
    <item name="android:background">@android:color/transparent</item>
    <item name="android:lineSpacingMultiplier">1.2</item>
    <item name="android:textColor">@android:color/white</item>
    <item name="android:textSize">14.0sp</item>
    <item name="android:maxLines">3</item>
    <item name="android:singleLine">false</item>
    <item name="android:paddingTop">4dp</item>
    <item name="android:paddingBottom">4dp</item>
    <item name="android:includeFontPadding">false</item>
  </style>
  <style name="Appboy.Slideup.Chevron">
    <item name="android:background">@drawable/com_appboy_slideup_drawer_arrow</item>
  </style>

  <!-- Feed -->
  <style name="Appboy.Feed"/>
  <!-- Styling the ListView background will require you to set the cacheColorHint, otherwise the
   background may appear black on older versions of Android. For more information about the issue,
   see http://android-developers.blogspot.com/2009/01/why-is-my-list-black-android.html -->
  <style name="Appboy.Feed.List">
    <item name="android:background">@android:color/transparent</item>
    <item name="android:divider">@android:color/transparent</item>
    <item name="android:dividerHeight">16.0dp</item>
    <item name="android:paddingLeft">12.5dp</item>
    <item name="android:paddingRight">5.0dp</item>
    <item name="android:scrollbarStyle">outsideInset</item>
  </style>
  <style name="Appboy.Feed.Empty">
    <item name="android:lineSpacingExtra">1.5dp</item>
    <item name="android:text">@string/com_appboy_feed_empty</item>
    <item name="android:textColor">@color/com_appboy_title</item>
    <item name="android:textSize">18.0sp</item>
  </style>
  <style name="Appboy.Feed.NetworkErrorTitle">
    <item name="android:gravity">center_horizontal</item>
    <item name="android:text">@string/com_appboy_feed_connection_error_title</item>
    <item name="android:textColor">@color/com_appboy_title</item>
    <item name="android:textSize">19.0sp</item>
    <item name="android:textStyle">bold</item>
  </style>
  <style name="Appboy.Feed.NetworkErrorBody">
    <item name="android:gravity">center_horizontal</item>
    <item name="android:text">@string/com_appboy_feed_connection_error_body</item>
    <item name="android:textColor">@color/com_appboy_title</item>
    <item name="android:textSize">15.0sp</item>
  </style>

  <!-- Cards -->
  <style name="Appboy.Cards"/>

  <!-- Banner Card -->
  <style name="Appboy.Cards.BannerImage"/>
  <style name="Appboy.Cards.BannerImage.Image">
  </style>

  <!-- Captioned Image Card -->
  <style name="Appboy.Cards.CaptionedImage"/>
  <style name="Appboy.Cards.CaptionedImage.Image">
  </style>
  <style name="Appboy.Cards.CaptionedImage.Title.Container">
    <item name="android:background">@color/com_appboy_card_title_container</item>
  </style>
  <style name="Appboy.Cards.CaptionedImage.Title">
    <item name="android:textColor">@color/com_appboy_title</item>
    <item name="android:textSize">17.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:includeFontPadding">false</item>
  </style>
  <style name="Appboy.Cards.CaptionedImage.Description">
    <item name="android:textColor">@color/com_appboy_description</item>
    <item name="android:textSize">15.0sp</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingBottom">8.0dp</item>
  </style>
  <style name="Appboy.Cards.CaptionedImage.Domain">
    <item name="android:textColor">@color/com_appboy_domain</item>
    <item name="android:textSize">13.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingBottom">10.0dp</item>
  </style>

  <!-- Text Announcement Card -->
  <style name="Appboy.Cards.TextAnnouncement"/>
  <style name="Appboy.Cards.TextAnnouncement.Title">
    <item name="android:lineSpacingExtra">1.5dp</item>
    <item name="android:textColor">@color/com_appboy_title</item>
    <item name="android:textSize">16.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingBottom">8.0dp</item>
  </style>
  <style name="Appboy.Cards.TextAnnouncement.Description">
    <item name="android:lineSpacingExtra">1.5dp</item>
    <item name="android:textColor">@color/com_appboy_description</item>
    <item name="android:textSize">13.0sp</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingBottom">8.0dp</item>
  </style>
  <style name="Appboy.Cards.TextAnnouncement.Domain">
    <item name="android:textColor">@color/com_appboy_domain</item>
    <item name="android:textSize">13.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingBottom">10.0dp</item>
  </style>

  <!-- Short News Card -->
  <style name="Appboy.Cards.ShortNews"/>
  <style name="Appboy.Cards.ShortNews.Image">
  </style>
  <style name="Appboy.Cards.ShortNews.Title">
    <item name="android:lineSpacingExtra">1.5dp</item>
    <item name="android:textColor">@color/com_appboy_title</item>
    <item name="android:textSize">16.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:singleLine">true</item>
    <item name="android:includeFontPadding">false</item>
  </style>
  <style name="Appboy.Cards.ShortNews.Description">
    <item name="android:lineSpacingExtra">1.5dp</item>
    <item name="android:textColor">@color/com_appboy_description</item>
    <item name="android:textSize">13.0sp</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingTop">8.0dp</item>
    <item name="android:paddingBottom">8.0dp</item>
  </style>
  <style name="Appboy.Cards.ShortNews.Domain">
    <item name="android:textColor">@color/com_appboy_domain</item>
    <item name="android:textSize">13.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:includeFontPadding">false</item>
    <item name="android:paddingBottom">10.0dp</item>
  </style>

  <!-- Cross Promotion Small Card -->
  <style name="Appboy.Cards.CrossPromotionSmall"/>
  <style name="Appboy.Cards.CrossPromotionSmall.Layout">
    <item name="android:background">@drawable/com_appboy_card_background</item>
  </style>
  <style name="Appboy.Cards.CrossPromotionSmall.Title">
    <item name="android:textColor">@color/com_appboy_title</item>
    <item name="android:textSize">16.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:singleLine">false</item>
    <item name="android:includeFontPadding">false</item>
  </style>
  <style name="Appboy.Cards.CrossPromotionSmall.Subtitle">
    <item name="android:lineSpacingExtra">1.5dp</item>
    <item name="android:textColor">@color/com_appboy_cross_promotion_card_subtitle</item>
    <item name="android:textSize">9.0sp</item>
    <item name="android:paddingTop">2.0dp</item>
  </style>
  <style name="Appboy.Cards.CrossPromotionSmall.Image">
  </style>
  <style name="Appboy.Cards.CrossPromotionSmall.RatingBar">
    <item name="android:paddingTop">2.0dp</item>
    <item name="android:paddingBottom">10.0dp</item>
  </style>
  <style name="Appboy.Cards.CrossPromotionSmall.ReviewCount">
    <item name="android:textColor">@color/com_appboy_cross_promotion_card_review_count</item>
    <item name="android:textSize">11.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:maxLines">1</item>
    <item name="android:singleLine">true</item>
    <item name="android:paddingLeft">2.0dp</item>
  </style>
  <style name="Appboy.Cards.CrossPromotionSmall.Price" parent="Appboy.Button">
    <item name="android:padding">0.0dp</item>
    <item name="android:textColor">@color/com_appboy_cross_promotion_card_price</item>
    <item name="android:textSize">13.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:background">@drawable/com_appboy_button</item>
  </style>
  <style name="Appboy.Cards.CrossPromotionSmall.RecommendationTab">
    <item name="android:paddingTop">7.0dp</item>
    <item name="android:paddingBottom">4.0dp</item>
    <item name="android:paddingLeft">12.0dp</item>
    <item name="android:paddingRight">12.0dp</item>
    <item name="android:background">@drawable/com_appboy_cross_promotion_small_card_recommended_tab_background</item>
    <item name="android:gravity">center</item>
    <item name="android:textColor">@color/com_appboy_cross_promotion_small_card_recommendation_tab_text</item>
    <item name="android:textSize">12.0sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:includeFontPadding">false</item>
  </style>
</resources>